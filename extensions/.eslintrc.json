{
	"env": {
		"browser": true,
		"es2022": true
	},
	"plugins": [
		"es-x"
	],
	"extends": [
		"plugin:es-x/restrict-to-es2018"
	],
	"parserOptions": {
		"ecmaVersion": 13
	},
	"rules": {
		"logical-assignment-operators": 0,
		"es-x/no-array-prototype-flat": 0
	},
	"settings": {
		"es-x": {
			"aggressive": true
		}
	}
}
