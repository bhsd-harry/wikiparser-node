{
	"name": "wikiparser-node",
	"version": "0.10.0",
	"description": "A Node.js parser for MediaWiki markup with AST",
	"keywords": [
		"mediawiki",
		"wikitext",
		"parser"
	],
	"homepage": "https://github.com/bhsd-harry/wikiparser-node/wiki",
	"bugs": {
		"url": "https://github.com/bhsd-harry/wikiparser-node/issues"
	},
	"license": "GPL-3.0",
	"author": "Bhsd",
	"files": [
		"/index.js",
		"/config/",
		"/i18n/",
		"/lib/",
		"/mixin/",
		"/parser/",
		"/src/",
		"/tool/",
		"/util/",
		"/typings/",
		"/dist/"
	],
	"types": "./dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bhsd-harry/wikiparser-node.git"
	},
	"scripts": {
		"tsc": "rm -r dist/ && /usr/local/bin/tsc && grep -rl '/typings' dist/ | xargs gsed -i 's|/typings|/../typings|g' && node ./bin/tsc.js",
		"doc": "node ./bin/doc.js",
		"toc": "node ./bin/toc.js",
		"test": "eslint . && ajv -s config/.schema.json -d 'config/*.json' --strict=true --strict-required=false",
		"real": "node test/real.js",
		"single": "node --prof test/single.js && node --prof-process isolate-0x*-v8.log > test/processed.txt && rm isolate-0x*-v8.log"
	},
	"devDependencies": {
		"@types/node": "^17.0.23",
		"eslint": "^8.30.0",
		"eslint-plugin-n": "^15.6.0",
		"eslint-plugin-regexp": "^1.11.0",
		"eslint-plugin-unicorn": "^45.0.2",
		"eslint-plugin-jsdoc": "^1.0.0",
		"eslint-plugin-json-es": "^1.5.7",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"typescript": "^4.6.3",
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"@typescript-eslint/parser": "^5.47.1",
		"ajv-cli": "^5.0.0",
		"request": "^2.88.2"
	},
	"engines": {
		"node": "^18.4.0"
	}
}
